{
    "name": "picklewagon/request-logger",
    "type": "library",
    "description": "Add logging to your laravel and lumen applications.",
    "keywords": [
        "picklewagon",
        "laravel",
        "lumen"
    ],
    "homepage": "https://github.com/picklewagon/request-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Harrison",
            "email": "josh@picklewagon.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.2",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Picklewagon\\RequestLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Picklewagon\\RequestLogger\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
